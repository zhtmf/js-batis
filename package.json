{
  "name": "js-batis",
  "version": "1.0.0",
  "description": "lightweight and generic SQL library for JavaScript",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "find ./test -name '*.ts' -exec ./node_modules/.bin/ts-mocha --exit {} ;",
    "test2": "find ./test -name '*.ts' -exec ./node_modules/.bin/ts-mocha --exit {} \\;",
    "coverage": "nyc --reporter=html npm run test",
    "coverage2": "nyc --reporter=html npm run test2",
    "test-mysql": "./node_modules/.bin/ts-mocha ./test/sql/mysql.ts --exit",
    "test-sqlite": "./node_modules/.bin/ts-mocha ./test/sql/sqlite.ts --exit",
    "test-pg": "./node_modules/.bin/ts-mocha ./test/sql/pg.ts --exit",
    "test-aggregation": "./node_modules/.bin/ts-mocha ./test/sql/aggregation.ts --exit"
  },
  "keywords": [
    "database",
    "mysql",
    "postgresql",
    "postgres",
    "pg",
    "db",
    "nodejs",
    "nodejs",
    "sqlite",
    "handlebars",
    "sql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zhtmf/js-batis.git"
  },
  "author": "Eddie Deng",
  "license": "MIT",
  "dependencies": {
    "handlebars": "4.7.7",
    "md5": "2.3.0",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">= 13.10.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/mocha": "9.0.0",
    "@types/mysql": "2.15.19",
    "@types/node": "^16.7.1",
    "chai": "4.3.4",
    "custom-env": "2.0.1",
    "log4js": "^6.3.0",
    "loglevel": "^1.7.1",
    "mocha": "9.2.0",
    "mysql2": "2.3.0",
    "nyc": "15.1.0",
    "pg": "8.7.1",
    "rootpath": "^0.1.2",
    "sinon": "^11.1.2",
    "sqlite3": "^5.0.2",
    "ts-mocha": "^9.0.2",
    "winston": "^3.3.3"
  }
}
